{
  "name": "react-spectre.css",
  "version": "0.5.3",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toshiaki61/react-spectre.css.git"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "test": "node scripts/test.js --env=jsdom",
    "flow": "flow",
    "prod": "node scripts/build-lib.js",
    "build": "tsc --build tsconfig.json",
    "clean": "rimraf dist",
    "postinstall": "yarn build"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ],
      "react"
    ],
    "plugins": [
      "flow-react-proptypes",
      "react-flow-props-to-prop-types",
      "transform-flow-strip-types",
      "transform-object-rest-spread",
      "transform-class-properties"
    ],
    "sourceMaps": true,
    "retainLines": true
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "jasmine": true
    },
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": [
      "flowtype"
    ],
    "rules": {
      "no-console": "off",
      "object-curly-newline": "off",
      "jsx-a11y/href-no-hash": "off",
      "jsx-a11y/label-has-for": [
        "error",
        {
          "required": {
            "every": [
              "id"
            ]
          },
          "allowChildren": false
        }
      ],
      "react/default-props-match-prop-types": "off"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@types/classnames": "^2.2.6",
    "@types/jest": "^23.3.7",
    "@types/lodash.noop": "^3.0.4",
    "@types/ramda": "^0.25.39",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "autoprefixer": "7.1.6",
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-flow-react-proptypes": "^24.0.1",
    "babel-plugin-react-flow-props-to-prop-types": "^0.15.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "classnames": "^2.2.6",
    "css-loader": "0.28.7",
    "date-fns": "^1.29.0",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-typescript": "^0.12.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "flow-copy-source": "^2.0.0",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "^23.6.0",
    "lodash.noop": "^3.0.1",
    "npm-run-all": "^4.1.3",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "prettier": "^1.14.3",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "promise": "8.0.1",
    "prop-types": "^15.6.1",
    "prop-types-extra": "^1.1.0",
    "raf": "3.4.0",
    "ramda": "^0.25.0",
    "react": "^16.5.2",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.5.2",
    "recompose": "^0.27.1",
    "resolve": "1.6.0",
    "rimraf": "^2.6.2",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "ts-jest": "^23.10.4",
    "ts-loader": "^5.2.2",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.3",
    "typescript-eslint-parser": "^20.0.0",
    "url-loader": "0.6.2",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "peerDependencies": {
    "classnames": "^2.2.6",
    "lodash.noop": "^3.0.1",
    "prop-types": "^15.6.1",
    "prop-types-extra": "^1.1.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0"
  }
}
