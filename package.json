{
  "name": "react-spectre.css",
  "version": "0.5.13",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toshiaki61/react-spectre.css.git"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "test": "node scripts/test.js",
    "build": "tsc --build tsconfig.build.json",
    "clean": "rimraf dist",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "deploy-storybook": "storybook-to-ghpages",
    "chromatic": "chromatic test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "react-intl-auto",
        {
          "removePrefix": "src/",
          "filebase": false
        }
      ]
    ]
  },
  "dependencies": {
    "date-fns": "^1.30.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "spectre.css": "0.5.8",
    "storybook-chromatic": "^1.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-typescript": "^7.1.0",
    "@personare/react-storybook-decorator-github-corner": "^0.1.5",
    "@storybook/addon-actions": "^4.1.7",
    "@storybook/addon-info": "^4.1.7",
    "@storybook/addon-knobs": "^4.1.7",
    "@storybook/addon-links": "^4.1.7",
    "@storybook/addon-options": "^4.1.7",
    "@storybook/addons": "^4.1.7",
    "@storybook/react": "^4.1.7",
    "@storybook/storybook-deployer": "^2.8.1",
    "@svgr/webpack": "4.1.0",
    "@types/classnames": "^2.2.7",
    "@types/http-proxy-middleware": "^0.19.2",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "@types/ramda": "^0.25.47",
    "@types/react": "16.7.20",
    "@types/react-dom": "16.0.11",
    "@types/recompose": "^0.30.2",
    "@types/redux-immutable-state-invariant": "^2.1.0",
    "@types/storybook-readme": "^4.0.0",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__addon-info": "^3.4.2",
    "@types/storybook__addon-knobs": "^4.0.0",
    "@types/storybook__addon-links": "^3.3.3",
    "@types/storybook__addon-options": "^4.0.0",
    "@types/storybook__react": "^4.0.0",
    "@types/webpack": "^4.4.23",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "babel-plugin-named-asset-import": "^0.3.0",
    "babel-plugin-react-intl-auto": "^1.5.0",
    "babel-preset-react-app": "^7.0.0",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.2",
    "css-loader": "2.1.0",
    "dotenv": "6.2.0",
    "dotenv-expand": "4.2.0",
    "eslint": "5.12.1",
    "eslint-config-react-app": "^3.0.6",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "3.2.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.12.4",
    "file-loader": "3.0.1",
    "fork-ts-checker-webpack-plugin-alt": "0.4.14",
    "fs-extra": "7.0.1",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-pnp-resolver": "1.0.2",
    "jest-resolve": "23.6.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.2.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.5.0",
    "postcss-safe-parser": "4.0.1",
    "prettier": "^1.15.3",
    "prettier-stylelint": "^0.4.2",
    "prop-types": "^15.6.2",
    "raf": "3.4.1",
    "react-dev-utils": "^7.0.1",
    "react-docgen-typescript": "^1.12.3",
    "react-docgen-typescript-loader": "^3.0.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-scripts": "^2.1.3",
    "react-styleguidist": "^8.0.6",
    "redux-immutable-state-invariant": "^2.1.0",
    "resolve": "1.9.0",
    "sass-loader": "7.1.0",
    "storybook-readme": "^4.0.5",
    "style-loader": "0.23.1",
    "stylelint": "^9.10.0",
    "stylelint-config-rational-order": "^0.0.2",
    "terser-webpack-plugin": "1.2.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.4",
    "url-loader": "1.1.2",
    "webpack": "4.28.4",
    "webpack-dev-server": "3.1.14",
