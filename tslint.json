{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier",
    "rxjs-tslint-rules"
  ],
  "rules": {
    "jsx-boolean-value": false,
    "object-literal-sort-keys": false,
    "no-console": false,
    "no-unused-variable": false,
    "interface-name": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js",
      "src/registerServiceWorker.js"
    ]
  }
}
